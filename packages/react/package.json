{
  "name": "@liquid-svg-glass/react",
  "version": "0.0.1",
  "description": "React components for creating stunning SVG liquid glass effects",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "bun run tsc && bun run copy-styles",
