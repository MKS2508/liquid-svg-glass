  "keywords": [
    "react",
    "svg",
    "glass",
    "glassmorphism",
    "liquid",
    "visual-effects",
    "components",
    "ui",
    "typescript",
    "gsap"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/liquid-svg-glass.git",
    "directory": "packages/react"
  },
  "homepage": "https://your-username.github.io/liquid-svg-glass",
  "bugs": {
    "url": "https://github.com/your-username/liquid-svg-glass/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@liquid-svg-glass/core": "workspace:*",
    "react": "^19.1.0",
    "gsap": "^3.13.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "typescript": "~5.8.3"
  },
